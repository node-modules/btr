{
  "name": "clet",
  "version": "0.6.0",
  "description": "Command Line E2E Testing",
  "type": "module",
  "main": "./lib/runner.js",
  "exports": "./lib/runner.js",
  "types": "./lib/index.d.ts",
  "author": "TZ <atian25@qq.com> (https://github.com/atian25)",
  "homepage": "https://github.com/node-modules/clet",
  "repository": "git@github.com:node-modules/clet.git",
  "dependencies": {
    "dirname-filename-esm": "^1.1.1",
    "dot-prop": "^7.2.0",
    "execa": "^6.1.0",
    "lodash.ismatch": "^4.4.0",
    "p-event": "^5.0.1",
    "strip-ansi": "^7.0.1",
    "strip-final-newline": "^3.0.0",
    "throwback": "^4.1.0",
    "trash": "^8.1.0"
  },
  "devDependencies": {
    "common-jest-preset": "^1.0.0",
    "jest-summary-reporter": "^0.0.2",
    "cross-env": "^7.0.3",
    "egg-ci": "^1.19.0",
    "enquirer": "^2.3.6",
    "eslint": "^7",
    "eslint-config-egg": "^9",
    "jest": "^28.1.0",
    "supertest": "^6.2.3"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "cov": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "14, 16, 18",
